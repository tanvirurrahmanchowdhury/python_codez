# at the end of plots
# Show legend for all subplots
    for ax in axs.flatten():
        ax.legend()

    plt.tight_layout()
    axs[0,1].ticklabel_format(useOffset=False)
    plt.savefig(os.path.join(data_path,'e_factor_1_auto.png'), dpi=300,bbox_inches="tight")
    plt.show()

# prevent warnings
import warnings
warnings.filterwarnings("ignore")
